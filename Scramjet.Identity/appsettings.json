{
    "ConnectionStrings": {
        "DefaultConnection": "__DefaultConnection__"
    },
    "StsConfig": {
        "StsServerIdentity": "StsServerIdentity__",
        "AngularClientUrl": "__AngularClientUrl__",
        "AngularClientIdTokenOnlyUrl": "__AngularClientIdTokenOnlyUrl__",
        "AngularClientIdTokenOnlySecureUrl": "__AngularClientIdTokenOnlySecureUrl__"
    },
    "AdminConfiguration": {
        "IdentityAdminBaseUrl": "http://localhost:9000",
        "IdentityAdminRedirectUri": "http://localhost:9000/signin-oidc",
        "IdentityServerBaseUrl": "http://localhost:5000"
    },
    "UseLocalCertStore": "__UseLocalCertStore__",
    "CertificateThumbprint": "__CertificateThumbprint__",
    "Serilog": {
        "MinimumLevel": "Error",
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "===> {Timestamp:HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
                }
            }
        ]
    }
}